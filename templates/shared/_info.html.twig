{% for rubrique, messages in app.flashes %}
    {#    Pour un for dans un tableau associatif :   #}
    {#           Key       Value                     #}
    {#    {% for rubrique, messages in app.flashes %}#}
    <div class="alert alert-{{ rubrique }}">
        {% for message in messages %}
            <p>{{ message }}</p>
        {% endfor %}
    </div>
{% endfor %}